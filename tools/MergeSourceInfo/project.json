{
    "version": "1.0.0",
    "packOptions": {
        "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
        "projectUrl": "https://github.com/dotnet/docfx",
        "authors": [
            "Microsoft"
        ],
        "owners": [
            "Microsoft"
        ]
    },
    "dependencies": {
        "Microsoft.DocAsCode.Common": "1.0.0",
        "Microsoft.DocAsCode.DataContracts.Common": "1.0.0",
        "Microsoft.DocAsCode.DataContracts.ManagedReference": "1.0.0"
    },
    "configurations": {
        "Debug": {
            "buildOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "buildOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "buildOptions": {
        "emitEntryPoint": true
    },
    "frameworks": {
        "net452": {
            "frameworkAssemblies": {
                "System.Runtime": ""
            }
        }
    }
}