{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "dependencies": {
    "Microsoft.DocAsCode.Build.Engine": "1.0.0",
    "Microsoft.DocAsCode.DataContracts.Common": "1.0.0",
    "Microsoft.DocAsCode.Glob": "1.0.0",
    "Microsoft.DocAsCode.Plugins": "1.0.0",
    "Microsoft.DocAsCode.Utility": "1.0.0",
    "Rx-Core": "2.2.5",
    "Rx-Interfaces": "2.2.5",
    "Rx-Linq": "2.2.5",
    "Rx-Main": "2.2.5",
    "Rx-PlatformServices": "2.2.5"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG",
          "TRACE"
        ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [
          "TRACE"
        ],
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "emitEntryPoint": true
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Runtime": "",
        "System.Runtime.Caching": "4.0.0.0"
      }
    }
  }
}