{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.DocAsCode.Build.Engine": "1.0.0",
    "Microsoft.DocAsCode.Dfm": "1.0.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Net": "4.0.0.0"
      }
    }
  }
}
