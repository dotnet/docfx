{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "dependencies": {
    "AutoMapper": "5.2.0",
    "Microsoft.DocAsCode.Build.Common": "1.0.0",
    "Microsoft.DocAsCode.Common": "1.0.0",
    "Microsoft.DocAsCode.DataContracts.Common": "1.0.0",
    "Microsoft.DocAsCode.YamlSerialization": "1.0.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0"
      }
    }
  },
  "publishOptions": {
    "include": [
      "docfx.plugins.config",
      "resources"
    ]
  }
}
