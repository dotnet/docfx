{
  "definitions": {
    "ContributionConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "showContributors": {
          "type": "boolean"
        },
        "showEdit": {
          "type": "boolean"
        },
        "repository": {
          "type": "string"
        },
        "gitCommitsTime": {
          "type": "string"
        },
        "excludedContributors": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "DocumentIdConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sourceBasePath": {
          "type": "string"
        },
        "siteBasePath": {
          "type": "string"
        },
        "depotMappings": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "directoryMappings": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "FileConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "include": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exclude": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "GitHubConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "authToken": {
          "type": "string"
        },
        "userCache": {
          "type": "string"
        },
        "userCacheExpirationInHours": {
          "type": "integer"
        },
        "resolveUsers": {
          "type": "boolean"
        }
      }
    },
    "HttpConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "query": {
          "type": "string"
        },
        "headers": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "LocalizationConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "defaultLocale": {
          "type": "string"
        },
        "mapping": {
          "type": "string",
          "enum": [
            "repository",
            "folder",
            "repositoryAndFolder"
          ]
        },
        "bilingual": {
          "type": "boolean"
        }
      }
    },
    "OutputConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "path": {
          "type": "string"
        },
        "json": {
          "type": "boolean"
        },
        "pdf": {
          "type": "boolean"
        },
        "uglifyUrl": {
          "type": "boolean"
        },
        "lowerCaseUrl": {
          "type": "boolean"
        },
        "copyResources": {
          "type": "boolean"
        },
        "maxErrors": {
          "type": "integer"
        }
      }
    }
  },
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "product": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "content": {
      "$ref": "#/definitions/FileConfig"
    },
    "output": {
      "$ref": "#/definitions/OutputConfig"
    },
    "globalMetadata": {},
    "baseUrl": {
      "type": "string"
    },
    "extend": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "fileMetadata": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {}
      }
    },
    "routes": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "contribution": {
      "$ref": "#/definitions/ContributionConfig"
    },
    "dependencies": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "redirections": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "redirectionsWithoutId": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "documentId": {
      "$ref": "#/definitions/DocumentIdConfig"
    },
    "rules": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "enum": [
          "off",
          "info",
          "warning",
          "error"
        ]
      }
    },
    "http": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/HttpConfig"
      }
    },
    "gitHub": {
      "$ref": "#/definitions/GitHubConfig"
    },
    "warningsAsErrors": {
      "type": "boolean"
    },
    "xref": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "localization": {
      "$ref": "#/definitions/LocalizationConfig"
    }
  }
}