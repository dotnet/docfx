{
  "definitions": {
    "ContributionConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "showContributors": {
          "type": "boolean"
        },
        "showEdit": {
          "type": "boolean"
        },
        "repository": {
          "type": "string"
        },
        "branch": {
          "type": "string"
        },
        "gitCommitsTime": {
          "type": "string"
        },
        "excludedContributors": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "DocumentIdConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sourceBasePath": {
          "type": "string"
        },
        "siteBasePath": {
          "type": "string"
        },
        "depotMappings": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "directoryMappings": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "FileConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "include": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exclude": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "GitConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "authToken": {
          "type": "string"
        }
      }
    },
    "GitHubConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "authToken": {
          "type": "string"
        },
        "userCache": {
          "type": "string"
        },
        "userCacheExpirationInHours": {
          "type": "integer"
        },
        "resolveUsers": {
          "type": "boolean"
        }
      }
    },
    "GlobConfig<JObject>": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "include": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exclude": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "value": {},
        "isGlob": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "HttpConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "secrets": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/HttpSecretConfig"
          }
        }
      }
    },
    "HttpSecretConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "baseUrl": {
          "type": "string"
        },
        "query": {
          "type": "string"
        },
        "headers": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "OutputConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "path": {
          "type": "string"
        },
        "json": {
          "type": "boolean"
        },
        "uglifyUrl": {
          "type": "boolean"
        },
        "copyResources": {
          "type": "boolean"
        },
        "maxErrors": {
          "type": "integer"
        }
      }
    },
    "RouteConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "source": {
          "type": "string"
        },
        "destination": {
          "type": "string"
        }
      }
    }
  },
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "product": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "defaultLocale": {
      "type": "string"
    },
    "content": {
      "$ref": "#/definitions/FileConfig"
    },
    "output": {
      "$ref": "#/definitions/OutputConfig"
    },
    "globalMetadata": {},
    "siteBasePath": {
      "type": "string"
    },
    "sourceBasePath": {
      "type": "string"
    },
    "needGeneratePdfUrlTemplate": {
      "type": "boolean"
    },
    "baseUrl": {
      "type": "string"
    },
    "extend": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "fileMetadata": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/GlobConfig<JObject>"
      }
    },
    "routes": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/RouteConfig"
      }
    },
    "contribution": {
      "$ref": "#/definitions/ContributionConfig"
    },
    "dependencies": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "redirections": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "redirectionsWithoutId": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "documentId": {
      "$ref": "#/definitions/DocumentIdConfig"
    },
    "rules": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "enum": [
          "off",
          "info",
          "warning",
          "error"
        ]
      }
    },
    "http": {
      "$ref": "#/definitions/HttpConfig"
    },
    "gitHub": {
      "$ref": "#/definitions/GitHubConfig"
    },
    "git": {
      "$ref": "#/definitions/GitConfig"
    },
    "warningsAsErrors": {
      "type": "boolean"
    },
    "buildInternalXrefMap": {
      "type": "boolean"
    },
    "xref": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}