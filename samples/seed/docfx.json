{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "assembly/bin/**/*.dll",
            "project/**/*.csproj",
            "solution/**/*.sln",
            "csharp/**/*.cs",
            "vb/**/*.vb"
          ],
          "exclude": [
            "project/Project.Core/**"
          ],
          "src": "dotnet"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml", "restapi/**" ]
      },
      {
        "files": [ "docs/markdown.md" ],
        "src": "../../docs/"
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "docfx seed website",
      "_appName": "Seed",
      "_enableSearch": true
    },
    "dest": "_site",
    "exportViewModel": true,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  },
  "pdf": {
    "content": [
      {
        "files": [ "obj/api/*.yml" ],
        "exclude": [ "**/toc.yml" ]
      },
      {
        "files": [ "articles/**/*.md", "*.md", "restapi/**" ]
      },
      {
        "files": [ "pdf/toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "wkhtmltopdf": {
      "filePath": "C:/Program Files/wkhtmltopdf/bin/wkhtmltopdf.exe",
      "additionalArguments": "--enable-local-file-access"
    },
    "dest": "_site_pdf",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
