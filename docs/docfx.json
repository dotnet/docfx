{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src/Microsoft.DocAsCode.App"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "tutorial/**/*.{md,yml}", "*.{md,yml}", "templates-and-plugins/**/*.{md,yml}" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**" ],
        "exclude": [ "_site/**", "**/obj/**", "**.meta" ]
      },
      {
        "files": "schemas/**schema.json",
        "src": ".."
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "docfx",
      "_appFooter": "Made with <a href='https://dotnet.github.io/docfx/'>docfx</a>"
    },
    "dest": "_site",
    "markdownEngineName": "markdig",
    "template": [
      "default",
      "custom_template"
    ]
  }
}
