{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Microsoft.DocAsCode.App/*.csproj",
            "src/Microsoft.DocAsCode.Dotnet/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**", "codesnippet/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "docfx",
      "_appName": "docfx"
    },
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ]
  }
}
