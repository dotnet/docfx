{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Microsoft.DocAsCode.App/*.csproj",
            "src/Microsoft.DocAsCode.Dotnet/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**", "codesnippet/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "docfx",
      "_appName": "docfx",
      "_appFooter": "<span>Supported by the <a href=\"https://dotnetfoundation.org\">.NET Foundation</a>. Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>",
      "_googleAnalyticsTagId": "G-Q5N6XJHEX5"
    },
    "markdownEngineProperties": {
      "alerts": {
        "TODO": "alert alert-secondary"
      }
    },
    "sitemap": {
      "baseUrl": "https://dotnet.github.io/docfx",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ]
  }
}
