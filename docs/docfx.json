{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src/Microsoft.DocAsCode.App"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "docfx",
      "_appFooter": "Made with <a href='https://dotnet.github.io/docfx/'>docfx</a>"
    },
    "dest": "_site",
    "template": [
      "default",
      "template"
    ]
  }
}
