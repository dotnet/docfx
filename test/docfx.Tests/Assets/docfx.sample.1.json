{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**" ], // `exclude` is also relative to `src`
          "src": "../src"
        }
      ],
      "dest": "obj/docfx/api/dotnet"
    },
    {
      "src": [
        {
          "files": ["**/*.js"],
          "src": "../src"
        }
      ],
      "dest": "obj/docfx/api/js" // throw error when dest is not unique
    }
  ],
  "build": { // input could be YAML or MARKDOWN files, outputs are Final-YAML files
    "content":
      [
        {
          "files": ["**/*.yml"],
          "src": "obj/docfx"
        },
        {
          "files": ["tutorial/**/*.md", "spec/**/*.md"]
        },
        {
          "files": ["toc.yml"]
        }
      ],
    "resource": [
        {
          "files": ["images/**"]
        }
    ],
    "globalMetadata": {
      "key": "value"
    },
    "fileMetadata": {
      "key": {
        "filepattern1": "string",
        "filePattern2": 2,
        "filePattern3": true,
        "filePattern4": [ ],
        "filePattern5": { }
      }
    },
    "overwrite": "apispec/*.md",
    "xref": [
      "external/*.yml.zip"
    ],
    "dest": "_site",
    "template": "default",
    "theme": "happy"
  }
}
