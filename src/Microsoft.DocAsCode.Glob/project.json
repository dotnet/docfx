{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  }
}