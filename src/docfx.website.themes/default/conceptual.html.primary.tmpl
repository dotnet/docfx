{{!include(/^styles/.*/)}}
{{!include(/^fonts/.*/)}}
{{!include(favicon.ico)}}
{{!include(logo.svg)}}
<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body data-spy="scroll" data-target="#affix">
    <div id="wrapper">
      <header>
        {{>partials/navbar}}
        {{>partials/breadcrumb}}
      </header>
      <div role="main" class="container body-content">
      {{#_allowToc}}
        {{>partials/toc}}
        <div class="article row grid-right">
      {{/_allowToc}}
      {{^_allowToc}}
        <div class="article row grid">
        {{/_allowToc}}
          {{#_allowAffix}}
          <div class="col-md-10">
          {{/_allowAffix}}
          {{^_allowAffix}}
          <div class="col-md-12">
          {{/_allowAffix}}
            <article class="content wrap" id="_content" data-uid="{{uid}}">
              <a href="{{docurl}}" class="btn btn-primary pull-right mobile-hide">Improve this Doc</a>
              {{{conceptual}}}
            </article>
          </div>
          {{#_allowAffix}}
          {{>partials/affix}}
          {{/_allowAffix}}
        </div>
      </div>
      {{>partials/footer}}
    </div>
    {{>partials/scripts}}
  </body>
</html>