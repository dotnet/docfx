<!-- CLASS -->
{{>partials/header}}
{{>partials/summary}}
<div class="level">
  <section class="primary">
    <div class="alert alert-minimal syntax">
      <h5><span class="icon icon-syntax"></span> Declaration</h5>
        {{#syntax}}
        <figure>
          <pre>{{#content}}<code class="{{lang}} lang-{{lang}}">{{value}}</code>{{/content}}</pre>
        </figure>
        {{/syntax}}
    </div>
  </section>
  <section class="secondary">
    <div class="alert alert-minimal inheritance">
      <h5><span class="icon icon-inheritance"></span> Inheritance Hierarchy</h5>
      {{#inheritance.0}}
      <ul>
      {{#inheritance}}
        <li class="l{{index}}">
          {{#specName}}
          <code class="lang-{{lang}}">
            {{{value}}}
          </code>
          {{/specName}}
        </li>
      {{/inheritance}}
        <li class="l{{item.level}}">
          <span class="xref">
            {{#item.name}}
            <code class="lang-{{lang}}">{{value}}</code>
            {{/item.name}}
          </span>
        </li>
      </ul>
      {{/inheritance.0}}
      {{^inheritance.0}}
      <p><code>None</code></p>
      {{/inheritance.0}}
    </div>
    <div class="alert alert-minimal">
      <h5><span class="icon icon-assembly"></span> Assembly</h5>
      {{#assemblies.0}}
      <p><code>{{assemblies.0}}.dll</code></p>
      {{/assemblies.0}}
      {{^assemblies.0}}
      <p><code>None</code></p>
      {{/assemblies.0}}
    </div>
  </section>
</div>
{{#conceptual}}
<header class="header" id="conceptual">
  <h2>Using</h2>
</header>
<div class="level">
  {{{conceptual}}}
</div>
{{/conceptual}}
{{#remarks}}
<header class="header" id="remarks">
  <h2>Remarks</h2>
</header>
<div class="level">
  <section class="primary">
    {{{remarks}}}
  </section>
</div>
{{/remarks}}
{{#children}}
<header class="header{{#platform}} platform-{{.}}{{/platform}} enable-platform-filter" id="{{id}}">
  {{^hideSubtitle}}
  <h2>{{>partials/classSubtitle}}</h2>
  {{/hideSubtitle}}
</header>
<ul class="list-clean{{#platform}} platform-{{.}}{{/platform}} enable-platform-filter">
  {{#children}}
  <li class="expand-container{{#platform}} platform-{{.}}{{/platform}} enable-platform-filter" id="{{id}}">
    <h4><a href class="expander expanded">
    {{#name}}
      <code class="lang-{{lang}}">{{value}}</code>
    {{/name}}
    </a></h4>
    <section class="content">
      <div class="level">
        <section class="primary">
        {{#summary}}
          <h6>Summary</h6>
          {{{summary}}}
        {{/summary}}
        </section>
        {{#syntax}}
        <section class="secondary">
          <h6>Declaration</h6>
          <figure>
            <pre>{{#content}}<code class="{{lang}} lang-{{lang}}">{{value}}</code>{{/content}}</pre>
          </figure>
        </section>
        {{/syntax}}
      </div>
      {{#syntax}}
      <div class="level">
        <section class="primary">
          {{#parameters.0}}
          <h6>Parameters</h6>
          <ul class="member-details">
          {{/parameters.0}}
          {{#parameters}}
            <li>
              <ol class="meta">
                <li><code>{{{id}}}</code></li>
                <li>
                  {{#type.specName}}
                    <code class="lang-{{lang}}">{{{value}}}</code>
                  {{/type.specName}}
                </li>
              </ol>
              <section class="description">
                {{{description}}}
              </section>
            </li>
          {{/parameters}}
          {{#parameters.0}}
          </ul>
          {{/parameters.0}}
        </section>
        <section class="secondary">
        {{#return}}
          <h6>Returns</h6>
          <ul class="member-details">
            <li>
              <ol class="meta">
                <li>
                {{#type.specName}}
                  <code class="lang-{{lang}}">{{{value}}}</code>
                {{/type.specName}}
                </li>
              </ol>
              <section class="description">
                {{{description}}}
              </section>
            </li>
          </ul>
        {{/return}}
        {{#propertyValue}}
          <h6>Property Value</h6>
          <ul class="member-details">
            <li>
              <ol class="meta">
                <li>
                {{#type.specName}}
                  <code class="lang-{{lang}}">{{{value}}}</code>
                {{/type.specName}}
                </li>
              </ol>
              <section class="description">
                {{{description}}}
              </section>
            </li>
          </ul>
        {{/propertyValue}}
        {{#exceptions.0}}
          <h6>Exceptions</h6>
          <ul class="member-details">
        {{/exceptions.0}}
        {{#exceptions}}
            <li>
              <ol class="meta">
                <li>
                {{#type.specName}}
                  <code class="lang-{{lang}}">{{{value}}}</code>
                {{/type.specName}}
                </li>
              </ol>
              <section class=description">
                {{{description}}}
              </section>
            </li>
        {{/exceptions}}
        {{#exceptions.0}}
          </ul>
        {{/exceptions.0}}
        </section>
      </div>
      {{/syntax}}
      <div class="level">
        <section class="primary">
        {{#remarks}}
          <h6>Remarks</h6>
          {{{remarks}}}
        {{/remarks}}
        </section>
        <section class="secondary">
        {{#example.0}}
          <h6>Example</h6>
          {{#example}}
          {{{.}}}
          {{/example}}
        {{/example.0}}
        </section>
      </div>
      {{#requirements_method.0}}
      <header class="header" id="requirements_method">
        <h6>Requirements</h6>
      </header>
      {{/requirements_method.0}}
      {{#requirements_method}}
      <div class="level">
      <section class="primary">
          <table class="table table-bordered table-striped table-condensed" border="1">
              <tbody>
                  <tr>
                  <td>{{{name}}}</td>
                  <td>{{{value}}}</td>
                  </tr>
              </tbody>
          </table>
      </section>
      <section class="secondary">
      </section>
      </div>
      {{/requirements_method}}
    </section>
  </li>
  {{/children}}
</ul>
{{/children}}
{{#defined_in}}
<header class="header" id="defined_in">
  <h2>Defined in</h2>
</header>
<div class="level">
  <section class="primary">
    {{{defined_in}}}
  </section>
</div>
{{/defined_in}}
{{#supported_platforms.0}}
<header class="header" id="supported_platforms">
  <h2>Supported Platforms</h2>
</header>
{{/supported_platforms.0}}
{{#supported_platforms}}
<div class="level">
  <section class="primary">
    <table class="table table-bordered table-striped table-condensed" border="1">
        <tbody>
            <tr>
            <td>{{{name}}}</td>
            <td>{{{value}}}</td>
            </tr>
        </tbody>
    </table>
  </section>
</div>
{{/supported_platforms}}
{{#requirements.0}}
<header class="header" id="requirements">
  <h2>Requirements</h2>
</header>
{{/requirements.0}}
{{#requirements}}
<div class="level">
  <section class="primary">
    <table class="table table-bordered table-striped table-condensed" border="1">
        <tbody>
            <tr>
            <td>{{{name}}}</td>
            <td>{{{value}}}</td>
            </tr>
        </tbody>
    </table>
  </section>
</div>
{{/requirements}}
{{#thread_safety}}
<header class="header" id="thread_safety">
  <h2>Thread Safety</h2>
</header>
<div class="level">
  <section class="primary">
    {{{thread_safety}}}
  </section>
</div>
{{/thread_safety}}
{{#seealso.0}}
<header class="header" id="seealso">
  <h2>See Also</h2>
</header>
{{/seealso.0}}
{{#seealso}}
<div class="level">
  <section class="primary">
     {{#type.specName}}
        <span class="lang-{{lang}}">{{{value}}}</span>
     {{/type.specName}}
  </section>
  <section class="secondary">
  </section>
</div>
{{/seealso}}
