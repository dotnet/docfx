{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "dependencies": {
    "Microsoft.DocAsCode.Build.Engine": "1.0.0",
    "Microsoft.DocAsCode.Plugins": "1.0.0"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {}
  },
  "buildOptions": {
    "emitEntryPoint": true
  }
}
