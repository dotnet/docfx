{
    "name": "DfmPreview",
    "displayName": "DfmPreview",
    "description": "preview the DocFX Flavored Markdown in vscode",
    "version": "0.0.9",
    "publisher": "928pjy",
    "engines": {
        "vscode": "^1.3.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:DFM.showpreview",
        "onCommand:DFM.showpreviewToside",
        "oncommand:DFM.showsource"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "DFM.showpreview",
                "title": "Toggle Preview"
            },
            {
                "command": "DFM.showpreviewToside",
                "title": "Open preview to the side"
            },
            {
                "command": "DFM.showsource",
                "title": "Show Source"
            }
        ],
        "keybindings": [
            {
                "command": "DFM.showpreview",
                "key": "ctrl+shift+q",
                "when": "editorTextFocus"
            },
            {
                "command": "DFM.showpreviewToside",
                "key": "ctrl+k q",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.14",
        "asyncawait": "1.0.6"
    }
}