{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "dependencies": {
    "Microsoft.DocAsCode.Build.Common": "1.0.0",
    "Microsoft.DocAsCode.Build.ConceptualDocuments": "1.0.0",
    "Microsoft.DocAsCode.Build.Engine": "1.0.0",
    "Microsoft.DocAsCode.Build.ManagedReference": "1.0.0",
    "Microsoft.DocAsCode.Build.ResourceFiles": "1.0.0",
    "Microsoft.DocAsCode.Build.RestApi": "1.0.0",
    "Microsoft.DocAsCode.Build.TableOfContents": "1.0.0",
    "Microsoft.DocAsCode.Common": "1.0.0",
    "Microsoft.DocAsCode.Dfm": "1.0.0",
    "Microsoft.DocAsCode.Plugins": "1.0.0",
    "Microsoft.DocAsCode.Metadata.ManagedReference": "1.0.0",
    "Microsoft.DocAsCode.Glob": "1.0.0",
    "CommandLineParser": "1.9.71",
    "Newtonsoft.Json": "9.0.1",
    "Owin": "1.0.0",
    "Microsoft.Owin": "3.0.1",
    "Microsoft.Owin.FileSystems": "3.0.1",
    "Microsoft.Owin.Host.HttpListener": "3.0.1",
    "Microsoft.Owin.Hosting": "3.0.1",
    "Microsoft.Owin.SelfHost": "3.0.1",
    "Microsoft.Owin.StaticFiles": "3.0.1"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {}
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "embed": {
      "include": [ "Template/**.zip" ],
      "mappings": {
        "langwordMapping.yml": "langwordMapping.yml"
      }
    }
  }
}
