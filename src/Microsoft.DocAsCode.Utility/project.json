{
  "version": "1.0.0",
  "packOptions": {
    "licenseUrl": "https://github.com/dotnet/docfx/blob/dev/LICENSE",
    "projectUrl": "https://github.com/dotnet/docfx",
    "authors": [
      "Microsoft"
    ],
    "owners": [
      "Microsoft"
    ]
  },
  "dependencies": {
    "GitSharp": "0.3.0",
    "Newtonsoft.Json": "9.0.1",
    "YamlDotNet.Signed": "3.9.0",
    "Microsoft.DocAsCode.Common": "1.0.0"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Web": {}
      }
    }
  }
}
